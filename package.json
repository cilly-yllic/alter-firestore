{
  "name": "alter-firestore",
  "type": "module",
  "version": "1.0.5",
  "description": "",
  "types": "./lib/src/cli/index.d.ts",
  "main": "./lib/src/cli/index.js",
  "bin": {
    "alfs": "./lib/src/cli/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "run-s build:{lint,clean,tsc,copy,generate}",
    "build:lint": "npm run lint",
    "build:clean": "npm run clean",
    "build:copy": "npm run copy",
    "build:tsc": "npm run tsc",
    "build:generate": "npm run generate",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext ts,tsx",
    "clean": "rimraf ./lib ./types ./utils",
    "tsc": "tsc --build tsconfig.publish.json",
    "copy": "cp -R ./src/templates ./lib/src",
    "generate": "node ./tools/generate-alias.cjs"
  },
  "engines": {
    "node": ">=19.0.0 <20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cilly-yllic/alter-firestore.git"
  },
  "keywords": [
    "cli",
    "firestore",
    "migration"
  ],
  "author": "cilly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cilly-yllic/alter-firestore/issues"
  },
  "homepage": "https://github.com/cilly-yllic/alter-firestore#readme",
  "exports": {
    "./types/*": "./lib/src/types/*.js",
    "./utils/*": "./lib/src/utils/*.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "commander": "^11.0.0",
    "firebase-admin": "^11.11.0",
    "strip-ansi": "^7.1.0",
    "triple-beam": "^1.4.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/triple-beam": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib",
    "types",
    "utils"
  ]
}
